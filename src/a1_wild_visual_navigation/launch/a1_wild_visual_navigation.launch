<launch>
    <!-- Arguments -->
    <arg name="camera_file"       default="$(find a1_wild_visual_navigation)/config/wild_visual_navigation/camera.yaml"/>
    <arg name="params_file"       default="$(find a1_wild_visual_navigation)/config/wild_visual_navigation/a1.yaml"/>
    <arg name="overlay_images"    default="True"/>

    <include file="$(find wild_visual_navigation_ros)/launch/wild_visual_navigation.launch">
        <arg name="camera_file"    value="$(arg camera_file)"/>
        <arg name="params_file"    value="$(arg params_file)"/>
        <arg name="overlay_images" value="$(arg overlay_images)"/>
    </include>

      <!-- a1 state converter required by WVN -->
  <node name="a1_state_converter_node" pkg="a1_wild_visual_navigation" type="a1_state_converter_node.py"/>

  </launch>
  